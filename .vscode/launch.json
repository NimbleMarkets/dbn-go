{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "dbn-go-hist",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/dbn-go-hist/main.go",
            "env": {
                "DATABENTO_API_KEY": "<key>"
            },
            "args": [
                "datasets",
            ]
        },
        {
            "name": "Attach dbn-go-hist TUI",
            "type": "go",
            "debugAdapter": "dlv-dap",
            "request": "attach",
            "mode": "remote",
            "remotePath": "${workspaceFolder}/cmd/dbn-go-hist",
            "port": 2345,
            "host": "127.0.0.1",
            "preLaunchTask": "Run TUI with headless dlv"
        },
        {
            "name": "dbn-go-live",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/dbn-go-live/main.go",
            "env": {
                "DATABENTO_API_KEY": "<key>",
            },
            "args": [
                "-v",
                "-d", "DBEQ.BASIC",
                "-s", "ohlcv-1m",
                "-o", "${workspaceFolder}/foo.launch.dbn",
                "MSFT"
            ]
        }
    ]
}
